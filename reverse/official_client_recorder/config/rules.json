      "image/",
      "video/",
      "audio/",
      "text/css",
      "font/",
      "application/font",
      "application/vnd.ms-fontobject"
    ],
    "urlPatterns": [
      "\\.(png|jpg|jpeg|gif|webp|svg|ico)(\\?.*)?$",
      "\\.(mp4|mp3|wav|ogg|avi)(\\?.*)?$",
      "\\.(css|woff2?|ttf|otf|eot)(\\?.*)?$",
      "/favicon\\.",
      "static\\.xx\\.fbcdn\\.net",
      "fbcdn\\.net/.*/images/",
